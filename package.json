{
    "name": "budget-app-client",
    "private": true,
    "version": "0.0.0",
    "author": "Kirill Korshunov (https://github.com/OneSiberianScript)",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc -b && vite build",
        "preview": "vite preview",
        "build:analyze": "vue-tsc -b && vite build --mode analyze",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "docs:api": "true",
        "prepare": "husky"
    },
    "lint-staged": {
        "*.{ts,vue}": [
            "eslint --fix",
            "prettier --write"
        ]
    },
    "dependencies": {
        "@vee-validate/zod": "^4.14.0",
        "@vicons/ionicons5": "^0.12.0",
        "@vueuse/core": "^11.2.0",
        "ant-design-vue": "^4.2.0",
        "axios": "^1.7.0",
        "echarts": "^5.5.0",
        "pinia": "^2.2.0",
        "vee-validate": "^4.14.0",
        "vue": "^3.5.0",
        "vue-echarts": "^7.0.0",
        "vue-router": "^4.4.0",
        "zod": "^3.23.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@types/node": "^22.0.0",
        "@vitejs/plugin-vue": "^5.2.0",
        "@vue/tsconfig": "^0.5.0",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-import-x": "^4.16.1",
        "eslint-plugin-vue": "^10.8.0",
        "globals": "^17.3.0",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.7",
        "prettier": "^3.8.1",
        "rollup-plugin-visualizer": "^5.12.0",
        "typescript": "~5.6.0",
        "typescript-eslint": "^8.56.0",
        "vite": "^6.0.0",
        "vue-tsc": "^2.1.0"
    }
}
